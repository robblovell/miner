<h1>Listing indices</h1>

<table>
  <thead>
    <tr>
      <th>Miner</th>
      <th>Mode</th>
      <th>Make</th>
      <th>Year</th>
      <th>Model</th>
      <th>Engine</th>
      <th>System</th>
      <th>Dtc</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @indices.each do |index| %>
      <tr>
        <td><%= index.miner %></td>
        <td><%= index.mode %></td>
        <td><%= index.make %></td>
        <td><%= index.year %></td>
        <td><%= index.model %></td>
        <td><%= index.engine %></td>
        <td><%= index.system %></td>
        <td><%= index.dtc %></td>
        <td><%= link_to 'Show', index %></td>
        <td><%= link_to 'Edit', edit_index_path(index) %></td>
        <td><%= link_to 'Destroy', index, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Index', new_index_path %>
